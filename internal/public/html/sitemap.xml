{{ safeHTML `<?xml version="1.0" encoding="utf-8"?>` }}
<urlset xmlns="http://www.w3.org/2005/Atom">
	{{ range .Urls }}
	<url>
		<loc>{{ .Uri }}</loc>
		{{ with .Fecha_actualizacion }}<lastmod>{{ . }}</lastmod>{{ end }}
		{{ with .Changefreq }}<changefreq>{{ . }}</changefreq>{{ end }}
		{{ with .Priority }}<priority>{{ . }}</priority>{{ end }}
	</url>
	{{ end }}
</urlset>
